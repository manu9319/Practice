package OopsConcept;
class Tester extends Emp
{
	private String Designation = " Tester "; 

	String getDesignation()
	{
		return Designation ; 
	}
	void setDesignation(String Designation)
	{
		this.Designation=Designation;
	}

	Tester()
	{
	}

	Tester ( String name , double salary , String Designation   )
	{
		super(name , salary) ; 
	}
	
}
