package OopsConcept;
class Emp 
{
	static int counter = 0 ;
	private int eID ; 
	private double salary ; 
	String name ; 

	{
		eID = ++counter ; 
	}

	int getEID ()
	{
		return eID ;
	}

	double getSalary()
	{
		return salary ; 
	}

	double setSalary( double salary)
	{
		this.salary = salary ; 
		return salary;
	}

	

	Emp()
	{
	}

	Emp( String name , double salary)
	{
		this.name = name ; 
		this.salary = salary ; 
	}
	
}

