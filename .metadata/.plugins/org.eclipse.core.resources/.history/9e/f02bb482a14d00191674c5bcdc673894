package OopsConcept;
class Ate extends Tester
{
	private String tool ; 

	String getTool()
	{
		return tool ; 
	}
	void setTool( String tool )
	{
		this.tool = tool ; 
		
	}

	Ate()
	{
	}

	Ate ( String name , double salary , String Designation,String tool  )
	{
		super(name,salary,Designation); 
		this.tool = tool ; 
	}
	void script()
		{
		System.out.println(name +" is working on" + tool) ; 

		}
	}

